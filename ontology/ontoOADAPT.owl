<rdf:RDF xmlns="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#"
     xml:base="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
