    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Activity -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Activity">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Context_of_Use"/>
        <rdfs:comment xml:lang="en">&quot;A userâ€™s activity may have a specific location, time and context. The activity context is used to describe information about the users, tasks and goals.&quot; (Iqbal et al., 2021). @Ex.: Task Type (Reading, Writing).</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">ContUsO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Adaptive_User_Interface -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Adaptive_User_Interface">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface"/>
        <rdfs:comment xml:lang="en">Focuses on the adaptive aspects of the user interface, managing the look and feel of the interface. Interface adapted to meet the needs of users. Based on user characteristics/difficulties.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Age -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Age">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Measurable_User_Profile_Element"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Alert_Component -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Alert_Component">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Component_Program"/>
        <rdfs:comment xml:lang="en">Represents alert components responsible for notifying the user about important events, required actions, or changes in the system. It can be presented in various formats, such as visual messages, audio, or vibration. @Ex.: Pop-up notifications, sound alerts, device vibrations, warning banners.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">UISCO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Audio_Input_Device -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Audio_Input_Device">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Input_Equipment"/>
        <rdfs:comment xml:lang="en">Devices to capture voice commands or sounds from the environment. @Ex.: a virtual assistant that adjusts the response volume depending on the noise captured by the microphone.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">HCIO Interactive Computer System Sub-Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Audio_Output_Device -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Audio_Output_Device">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Output_Equipment"/>
        <rdfs:comment xml:lang="en">Devices that allow interaction through audio feedback, such as sound alerts, speech output, and music playback. @Ex.: Speakers providing voice notifications in virtual assistants.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">HCIO Interactive Computer System Sub-Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Auditory_Disability -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Auditory_Disability">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Disability"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Average_Experience_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Average_Experience_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Basic_Experience_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Basic_Experience_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <owl:disjointWith rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#High_Experience_Mode"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Button_Component -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Button_Component">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Component_Program"/>
        <rdfs:comment xml:lang="en">An interactive UI element designed to perform an action or trigger an event when the user interacts with it (usually by click, tap, or voice command). It can be represented by text, icons, or images, and can have different visual states to indicate its availability (active, deactivated, pressed). @Ex.: &quot;Submit&quot; button in forms, &quot;Play/Pause&quot; button in media players, &quot;Like&quot; icon in social networks.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">UISCO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Captions_and_Transcripts_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Captions_and_Transcripts_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Cognitive_Disability -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Cognitive_Disability">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Disability"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Context_of_Use -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Context_of_Use">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#activates"/>
                <owl:someValuesFrom rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#includes"/>
                <owl:someValuesFrom rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Profile_Element"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">&quot;Devices are used in various kinds of situation which changes dynamically according to particular context change. Small screen size, display, mobility, sensor, capacity, processor, memory, graphics, input and output functionalities affect the usage of interfaces and interaction&quot;. (IQBAL, 2021)</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">ContUsO Ontology</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="en">subtypeOf Situation</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Contrast_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Contrast_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <owl:disjointWith rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Light_Mode"/>
        <rdfs:comment xml:lang="en">Some people need high contrast between text and background, including many older people who lose contrast sensitivity from ageing. Some read better with dark text on light background. W3C Web Accessibility Initiative (WAI), 2025.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Dark_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Dark_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <owl:disjointWith rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Light_Mode"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Desktop_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Desktop_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <owl:disjointWith rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Mobile_Mode"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Disability -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Disability">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Nominal_User_Profile_Element"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Education_Level -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Education_Level">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Measurable_User_Profile_Element"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Environment -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Environment">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Context_of_Use"/>
        <rdfs:comment xml:lang="en">It includes information about the physical environment in which the device is located, such as temperature, humidity, lighting, geographic location, among other relevant environmental factors. @Ex.: Light Level (Low, Medium, High), Noise Level (Quiet, Loud), Time Of Day (Day, Night).</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">ContUsO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Experience_Level -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Experience_Level">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Measurable_User_Profile_Element"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Hardware_Equipment -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Hardware_Equipment">
        <owl:disjointWith rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User"/>
        <rdfs:isDefinedBy xml:lang="en">SysSwO</rdfs:isDefinedBy>
        <UFO xml:lang="en">subtypeOf Object</UFO>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#High_Experience_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#High_Experience_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Input_Equipment -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Input_Equipment">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Hardware_Equipment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#presentAt"/>
                <owl:someValuesFrom rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Context_of_Use"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Hardware Equipment used to receive and transmit information or data from a User (input). E.g.: keyboard, mouse, sensors.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">HCIO Interactive Computer System Sub-Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Interaction_Adaptation_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Interaction_Adaptation_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Layout_Component -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Layout_Component">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Component_Program"/>
        <rdfs:comment xml:lang="en">Structural UI element responsible for visually organizing and distributing other components (such as buttons, texts and images) in a logical and functional way. It defines the hierarchy, positioning and alignment of UI elements. @Ex.: product grid in e-commerce, column layout on websites, side navigation panels in applications, photo grid in image galleries and dashboard with organized graphics.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">UISCO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Light_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Light_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <rdfs:comment xml:lang="en">Some people need low brightness, especially for backgrounds. Some people who need low brightness for backgrounds also need low brightness overall and thus need low brightness text. W3C Web Accessibility Initiative (WAI), 2025.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Measurable_User_Profile_Element -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Measurable_User_Profile_Element">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Profile_Element"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Media_Component -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Media_Component">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Component_Program"/>
        <rdfs:comment xml:lang="en">Represents any multimedia element that composes or integrates a system or interface. @Ex.: Video components: multimedia element for displaying videos; Images: component for displaying static images; Text elements: displaying plain or formatted text.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">UISCO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Mobile_Mode -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Mobile_Mode">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Interface_Customization"/>
        <rdfs:isDefinedBy xml:lang="en">AUISO Ontology</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Nominal_User_Profile_Element -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Nominal_User_Profile_Element">
        <rdfs:subClassOf rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#User_Profile_Element"/>
    </owl:Class>
    


    <!-- urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Output_Equipment -->

    <owl:Class rdf:about="urn:absolute:dev.nemo.inf.ufes.br/oadapt/ontoOADAPT.owl#Output_Equipment">
